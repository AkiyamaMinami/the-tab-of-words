{
  "name": "言葉 の Tab",
  "description": "A minimal Chrome extension to help you study Japanese words in each new tab.",
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "dist/index.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "manifest_version": 2,
  "permissions": [
    "storage"
  ]
}
